{
    "include": ["electron.vite.config.*", "src/main/**/*", "src/preload/**/*", "src/renderer/src/**/*"],
    "compilerOptions": {
      "jsx": "react-jsx",
      "strict": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
    },
  }